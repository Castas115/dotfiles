output "LG Display 0x0555 0x000492A0"{
	mode 2736x1824@59.95900
	position 300,1440
	alias $SURFACE
}

output "LG Electronics LG ULTRAGEAR 306NTDV47282" {
	mode 2560x1440@164.95799
	position 0,0
	alias $HOME_2K
}

profile s_home {
	output $SURFACE enable
	output $HOME_2K enable
}

profile s_laptop {
	output $SURFACE enable
	output $HOME_2K disable
}

